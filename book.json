{
    "title" : "Gavin博客",
    "description": "Gavin博客",
    "language" : "zh-hans",
    "author": "zhishengqianjun@163.com",
    "links": {
          "sharing": {
              "all": null,
              "facebook": null,
              "google": null,
              "twitter": null,
              "weibo": null
          }
    },
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-lunr",
        "-search",
        "-highlight",
        "-livereload",
        "copy-code-button",
	    "expandable-chapters-small",
        "html5-video",
        "splitter",
        "search-plus@^0.0.11",
        "simple-page-toc@^0.1.1",
        "edit-link@^2.0.2",
        "html5-video",
        "advanced-emoji@^0.2.1",
        "anchors@^0.7.1",
        "include-codeblock@^3.0.2",
        "ace@^0.3.2",
        "emphasize@^1.1.0",
        "katex@^1.1.3",
        "mermaid-gb3@2.1.0",
        "sectionx@^3.1.0",
        "local-video@^1.0.1",
        "anchor-navigation-ex@0.1.8",
        "todo@^0.1.3",
        "terminal@^0.3.2",
        "alerts@^0.2.0",
        "include-csv@^0.1.0",
        "puml@^1.0.1",
        "versions-select@^0.1.1",
        "-sharing",
        "graph@^0.1.0",
        "chart@^0.2.0",
	    "flowchart-full"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
	"expandable-chapters-small":{},
        "edit-link": {
            "base": "https://github.com/gaviners/gaviners.github.io/blob/gitbook_blog",
            "label": "Edit This Page"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
	"flow": {
	    "line-color": "red"
        },
        "sectionx": {
            "tag": "b"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    }
}
